<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
	<session-factory>
		<property name="hibernate.connection.driver_class">org.postgresql.Driver</property>
		<property name="hibernate.connection.url">jdbc:postgresql://localhost:5432/smz</property>
		<property name="hibernate.connection.username">smz</property>
		<property name="hibernate.connection.password">smz</property>
		<property name="hibernate.dialect">org.hibernate.dialect.PostgreSQLDialect</property>
		<property name="connection_pool_size">1</property>
		<property name="show_sql">true</property>
		<property name="format_sql">true</property>
		<mapping resource="com/mkyong/stock/Stock.hbm.xml"/>
		<mapping resource="com/mkyong/stock/StockDailyRecord.hbm.xml"/>
		<mapping resource="com/mkyong/stock/Adres.hbm.xml"/>
		<mapping resource="com/mkyong/stock/DaneOsobowe.hbm.xml"/>
		<mapping resource="com/mkyong/stock/KomorkaOrganizacyjna.hbm.xml"/>
		<mapping resource="com/mkyong/stock/PraktykaZawodowa.hbm.xml"/>
		<mapping resource="com/mkyong/stock/PraktykaZawodowaOsoba.hbm.xml"/>
		<mapping resource="com/mkyong/stock/Uslugodawca.hbm.xml"/>
		<mapping resource="com/mkyong/stock/SpecjalnoscKomorkiOrganizacyjnej.hbm.xml"/>
		<mapping resource="com/mkyong/stock/SKraj.hbm.xml"/>
		<mapping resource="com/mkyong/stock/SPlec.hbm.xml"/>
		<mapping resource="com/mkyong/stock/DaneKontaktowe.hbm.xml"/>
		<mapping resource="com/mkyong/stock/PrzedsiebiorstwoPodmiotuLeczniczego.hbm.xml"/>
		<mapping resource="com/mkyong/stock/JednostkaOrganizacyjna.hbm.xml"/>
		<mapping resource="com/mkyong/stock/PodmiotWDL.hbm.xml"/>
		<mapping resource="com/mkyong/stock/Wniosek.hbm.xml"/>
		<mapping resource="com/mkyong/stock/Organizacja.hbm.xml"/>
		<mapping resource="com/mkyong/stock/ZalacznikWniosku.hbm.xml"/>
		<mapping resource="com/mkyong/stock/ParametryKonfiguracyjne.hbm.xml"/>
	</session-factory>
</hibernate-configuration>

